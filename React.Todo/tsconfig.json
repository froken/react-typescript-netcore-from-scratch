{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": false,
      "noEmitOnError": true,
      "removeComments": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true
    },
    "exclude": [
        "node_modules",
        "build"
      ]
  }